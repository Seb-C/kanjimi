{
  "scripts": {
    "server:start": "ts-node src/server.ts",
    "server:watch": "nodemon -e ts -w ./src -x npm run server:start",
    "migrate": "ts-node src/migrate.ts",
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "lint": "tslint './src/*.ts' './tests/*.ts'",
    "extension:browser": "web-ext --config=web-ext.js run",
    "extension:build": "webpack --watch",
    "extension:watch": "webpack --watch & (npm run extension:browser) && fg"
  },
  "dependencies": {
    "@types/express": "^4.16.1",
    "@types/pg-query-stream": "^1.0.2",
    "express": "^4.16.4",
    "pg-promise": "^8.5.5",
    "pg-query-stream": "^2.0.0"
  },
  "devDependencies": {
    "@types/jasmine": "^3.3.9",
    "@types/node": "^11.9.3",
    "jasmine": "^3.3.1",
    "nodemon": "^1.18.10",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.3.3",
    "web-ext": "^3.0.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  }
}
